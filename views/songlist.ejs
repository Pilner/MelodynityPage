	<!-- NAV BAR -->

	<!-- TABLE -->
	<div class="container listbody">
		<div class="my-5">
			<table class="crop">
				<tr>
					<th>#</th>
					<th>Title</th>
					<th>Description</th>
					<th>Song URL</th>
					<th>Embed URL</th>
				</tr>
				<% songInfo.forEach((song, index) => { %>
				<tr>	
					<td><%= index + 1 %></td>
					<td><%= song["title"] %></td>
					<td><%= song["description"] %></td>
					<td class="wordbr"><%= song["songLink"] %></td>
					<td class="wordbr"><%= song["embedLink"] %></td>
				</tr>
				<% }) %>
			</table>
		</div>
	</div>
	<!-- TABLE -->
		
		<%- include("./partials/footer") %>